// p.87 switch문

package p220125;

public class Day04_06 {

	public static void main(String[] args) {
		
		int num = 3;
		
		switch (num) {
			case 1 :
				System.out.println("1번");
				break;
			case 2 :
				System.out.println("2번");
			case 3 :
				System.out.println("3번");
			case 4 :
				System.out.println("4번");
			case 5 :
				System.out.println("5번");
			default :
				System.out.println("6번");
		}
	}

}
